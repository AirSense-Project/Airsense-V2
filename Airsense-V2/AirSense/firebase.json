{
  "hosting": {
    "public": "AirSense",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**", 
        "function": "api"
      }
    ],
    "headers": [
      {
        "source": "/api/**",
        "headers": [
          {
            "key": "x-backend-url",
            "value": "https://airsense-v2.onrender.com" 
          }
        ]
      }
    ]
  }
}